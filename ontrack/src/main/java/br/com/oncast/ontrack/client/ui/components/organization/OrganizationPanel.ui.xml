<!DOCTYPE ui:UiBinder SYSTEM "http://dl.google.com/gwt/DTD/xhtml.ent">
<ui:UiBinder xmlns:ui="urn:ui:com.google.gwt.uibinder"
	xmlns:g="urn:import:com.google.gwt.user.client.ui"
	xmlns:layout="urn:import:br.com.oncast.ontrack.client.ui.generalwidgets.layout"
	xmlns:w="urn:import:br.com.oncast.ontrack.client.ui.generalwidgets">
	
	<ui:style src="../../style/default.css">
	
		.projectsContainer {
			position: absolute;
			
			top: PADDING_LARGE;
			left: PADDING_LARGE;
			right: PADDING_LARGE;
			bottom: PADDING_LARGE;
			
			overflow-x: hidden;
			overflow-y: auto;
		}
		
	</ui:style>
	
	
	<layout:ApplicationMenuAndWidgetContainer ui:field="rootPanel">
		<g:HTMLPanel styleName="{style.projectsContainer}">
			<w:ModelWidgetContainer ui:field="projects"/>
		</g:HTMLPanel>
	</layout:ApplicationMenuAndWidgetContainer>
</ui:UiBinder> 