<!DOCTYPE ui:UiBinder SYSTEM "http://dl.google.com/gwt/DTD/xhtml.ent">
<ui:UiBinder xmlns:ui="urn:ui:com.google.gwt.uibinder" xmlns:g="urn:import:com.google.gwt.user.client.ui">
	
	<ui:style src="../style/decorations.css, ../style/colors.css">

		.separator {
			margin: 10px -16px;
		}

		.border1 {
			background: COLOR_MENUBOX_SEPARATOR_BORDER1;
		}
		
		.border2 {
			background: COLOR_MENUBOX_SEPARATOR_BORDER2;
		}

	</ui:style>

	<g:HTMLPanel styleName="{style.separator}">
		<div class="{style.border1} {style.borderDecoration}" />
		<div class="{style.border2} {style.borderDecoration}" />
	</g:HTMLPanel>
</ui:UiBinder> 