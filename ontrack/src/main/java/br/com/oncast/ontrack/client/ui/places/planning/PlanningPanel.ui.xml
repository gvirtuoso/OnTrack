<!DOCTYPE ui:UiBinder SYSTEM "http://dl.google.com/gwt/DTD/xhtml.ent">
<ui:UiBinder xmlns:ui="urn:ui:com.google.gwt.uibinder"
	xmlns:g="urn:import:com.google.gwt.user.client.ui"
	xmlns:rp='urn:import:br.com.oncast.ontrack.client.ui.components.releasepanel'
	xmlns:st='urn:import:br.com.oncast.ontrack.client.ui.components.scopetree'>
	
	<g:SplitLayoutPanel height="400px">
		<g:west size="800">
			<st:ScopeTree ui:field="scopeTree"></st:ScopeTree>
		</g:west>
		<g:east size="400">
			<rp:ReleasePanel ui:field="releasePanel"></rp:ReleasePanel>
		</g:east>
	</g:SplitLayoutPanel>	

</ui:UiBinder>