<!DOCTYPE ui:UiBinder SYSTEM "http://dl.google.com/gwt/DTD/xhtml.ent">
<ui:UiBinder xmlns:ui="urn:ui:com.google.gwt.uibinder" xmlns:g="urn:import:com.google.gwt.user.client.ui">

	<ui:style src="../../style/default.css">
	
		.boardContainer {
		}
		
		.board {
			height: 100%; 
			margin-left: 10px;
		}
		
		.board > tbody > tr > td {
			height: 100%;
		}
		.board > tbody > tr > td > table, .board > tbody > tr > td > table > tbody > tr > td {
			height: 100%;
		}
	</ui:style>

	<g:HTMLPanel styleName="{style.boardContainer}">
		<g:HorizontalPanel ui:field="board" styleName="{style.board}" verticalAlignment="top" />
	</g:HTMLPanel>
</ui:UiBinder> 