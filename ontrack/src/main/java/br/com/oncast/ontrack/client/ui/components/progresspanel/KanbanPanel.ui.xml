<!DOCTYPE ui:UiBinder SYSTEM "http://dl.google.com/gwt/DTD/xhtml.ent">
<ui:UiBinder xmlns:ui="urn:ui:com.google.gwt.uibinder" xmlns:g="urn:import:com.google.gwt.user.client.ui">
	<ui:style>
		.panel {
			position: absolute;
			top: 0px;
			bottom: 0px;
			left: 0px;
			right: 0px;
			display: block;
			background: #fff;
		}
	</ui:style>

	<g:HorizontalPanel ui:field="board" styleName="{style.panel}" />
</ui:UiBinder> 