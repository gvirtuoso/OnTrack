<!DOCTYPE ui:UiBinder SYSTEM "http://dl.google.com/gwt/DTD/xhtml.ent">
<ui:UiBinder xmlns:ui="urn:ui:com.google.gwt.uibinder"
	xmlns:g="urn:import:com.google.gwt.user.client.ui">
	
	<ui:style type="br.com.oncast.ontrack.client.ui.generalwidgets.ReleaseCommandMenuItem.ReleaseCommandMenuItemStyle">
	
		.path {
			font-size: 12px;
			color: #777;
		}
		
		.lastName, .singleName {
			font-size: large;
			margin-top: 3px;
			color: #468;
		}
		
		.singleName {
			line-height: 42px;
			margin-top: 0px;
		}
		
	</ui:style>
	
	<g:HTMLPanel>
		<span ui:field="path" class="{style.path}"/>
		<div ui:field="name" class="{style.lastName}"/>
	</g:HTMLPanel>
	
</ui:UiBinder> 