<!DOCTYPE ui:UiBinder SYSTEM "http://dl.google.com/gwt/DTD/xhtml.ent">
<ui:UiBinder xmlns:ui="urn:ui:com.google.gwt.uibinder"
	xmlns:g="urn:import:com.google.gwt.user.client.ui"
	xmlns:gw='urn:import:br.com.oncast.ontrack.client.ui.generalwidgets'
	xmlns:ow='urn:import:br.com.oncast.ontrack.client.ui.components.progresspanel.widgets'
	>
	
	<ui:image field="image" src="newColumnImg.jpg" />
	<ui:style>
		.rootPanel {
			width: 16px;
			height: 16px;
		}
	</ui:style>
	
	<g:FocusPanel ui:field="rootPanel" styleName="{style.rootPanel}">
		<g:Image ui:field="create" resource="{image}" />
	</g:FocusPanel>
</ui:UiBinder> 