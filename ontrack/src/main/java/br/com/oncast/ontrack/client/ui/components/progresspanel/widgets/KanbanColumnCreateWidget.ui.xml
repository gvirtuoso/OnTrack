<!DOCTYPE ui:UiBinder SYSTEM "http://dl.google.com/gwt/DTD/xhtml.ent">
<ui:UiBinder xmlns:ui="urn:ui:com.google.gwt.uibinder"
	xmlns:g="urn:import:com.google.gwt.user.client.ui"
	xmlns:gw='urn:import:br.com.oncast.ontrack.client.ui.generalwidgets'
	xmlns:ow='urn:import:br.com.oncast.ontrack.client.ui.components.progresspanel.widgets'
	>
	
	<ui:style>
		.rootPanel {
			padding: 2px;
			margin: 2px 4px;
			width: 8px;
			height: 8px;
		}
	</ui:style>
	
	<g:FocusPanel ui:field="rootPanel" styleName="{style.rootPanel}">
		<g:Label ui:field="create">+</g:Label>
	</g:FocusPanel>
</ui:UiBinder> 